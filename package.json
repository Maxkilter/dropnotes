{
  "name": "dropnotes",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": "14.7.0",
    "npm": "6.14.7",
    "yarn": "1.22.4"
  },
  "main": "app.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node app.js",
    "server": "nodemon app.js",
    "client": "yarn --cwd client start",
    "client:install": "yarn --cwd client install",
    "client:build": "yarn --cwd client run build",
    "build": "yarn run client:build",
    "dev": "cross-env NODE_ENV=development concurrently \"yarn run server\" \"yarn run client\"",
    "restart": "concurrently \"npx kill-port 5000\" \"yarn run dev\""
  },
  "keywords": [
    "react",
    "notes"
  ],
  "author": "Max Boudnik <maxkilter@yahoo.com>",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "config": "^3.3.2",
    "express": "^4.17.1",
    "express-validator": "^6.6.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.9",
    "react-scripts": "^4.0.1"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "nodemon": "^2.0.6",
    "prettier": "2.1.2"
  }
}
