{
  "name": "dropnotes",
  "version": "7.0.0",
  "description": "",
  "scripts": {
    "start": "yarn --cwd server start",
    "server": "yarn --cwd server dev",
    "client": "yarn --cwd client start",
    "dev": "yarn run server & yarn run client",
    "client:install": "yarn --cwd client install",
    "server:install": "yarn --cwd server install",
    "install-app": "yarn client:install && yarn server:install",
    "clear-port": "npx kill-port 8000",
    "deploy": "yarn --cwd client build && yarn --cwd server start",
    "deploy-cluster": "yarn --cwd client build && yarn --cwd server cluster",
    "prettier": "npx prettier . --check",
    "prettier:fix": "npx prettier . --write",
    "reinstall-app": "rm -rf client/node_modules && rm -rf server/node_modules && yarn install-app"
  },
  "keywords": [
    "react",
    "notes"
  ],
  "author": "Max Boudnik <maxkilter@yahoo.com>",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "prettier": "2.8.8"
  }
}
