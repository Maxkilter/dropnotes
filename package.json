    "@types/react-dom": "^16.9.8",
    "lodash": "^4.17.20",
    "node-sass": "4.14.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "web-vitals": "^0.2.4"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "nodemon": "^2.0.6",
    "prettier": "2.1.2"
  }
}
